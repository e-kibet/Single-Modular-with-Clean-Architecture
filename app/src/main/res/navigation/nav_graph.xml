<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/viewBindingFragment">

    <fragment
        android:id="@+id/viewBindingFragment"
        android:name="com.example.viewanddatabindingandroidmvvm.ui.fragments.ViewBindingFragment"
        android:label="view binding"
        tools:layout="@layout/fragment_view_binding" >
        <action
            android:id="@+id/action_from_viewBindingFragment_to_dataBindingFragment"
            app:destination="@id/dataBindingFragment" />
    </fragment>
    <fragment
        android:id="@+id/dataBindingFragment"
        android:name="com.example.viewanddatabindingandroidmvvm.ui.fragments.DataBindingFragment"
        android:label="data binding"
        tools:layout="@layout/fragment_data_binding" />

</navigation>